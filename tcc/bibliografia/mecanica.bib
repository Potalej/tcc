@book{deriglazov_formalismo_2009,
	title = {Formalismo hamiltoniano e transformações canônicas em mecânica clássica},
	isbn = {978-85-7861-024-1},
	abstract = {O formalismo hamiltoniano da mecânica clássica serve como pano de fundo para a teoria quântica e também para várias áreas da física-matemática. Este livro cobre os tópicos ligados com a formulação hamiltoniana de mecância clássica, ao nível de graduação, mas com abordagem e nível de rigor para torná-lo útil também para alunos de pós-graduação. Sumário: 1- Formalismo Hamiltoniano 2- Transformações canônicas no espaço de fase bidimensional 3- Propriedades das transformações canônicas 4- Invariantes Integrais 5- Geometrização Formal da Mecânica 6- Transformações, mudança de variáveis, simetrias e teorema de Noether},
	language = {pt-BR},
	publisher = {Editora Livraria da Fisica},
	author = {Deriglazov, A. A. and Filgueiras, J. G.},
	year = {2009},
	note = {Google-Books-ID: XdBzVMFIDzMC},
	keywords = {Science / System Theory},
}
@book{lopes_introducao_2013,
	edition = {1ª edição - 1ª reimpressão},
	title = {Introdução à {Mecânica} {Clássica}},
	isbn = {978-85-314-0956-1},
	abstract = {Este livro aborda a mecânica newtoniana, lagrangiana e hamiltoniana do ponto de vista matemático e com uma abordagem elementar. Dirigido a alunos de graduação e pós-graduação em matemática, os conceitos são introduzidos de maneira natural e sempre acompanhados de exemplos que elucidam o assunto em discussão. As definições são matematicamente precisas e buscam apresentar as ideias fundamentais de maneira transparente. Ao final de cada capítulo, vários exercícios são propostos. Trata-se de livro de grande utilidade para uma primeira leitura matematicamente rigorosa da mecânica clássica.},
	language = {Português},
	publisher = {EDUSP},
	author = {Lopes, Artur O.},
	month = may,
	year = {2013},
}